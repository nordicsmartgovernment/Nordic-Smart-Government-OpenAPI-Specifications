{
  "openapi": "3.1.0",
  "info": {
    "version": "2.0.0",
    "title": "Nordic Governments API - business information",
    "termsOfService": "https://example.com/terms/",
    "contact": {
      "email": "contact@example.com",
      "url": "http://example.com/contact"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    },
    "x-logo": {
      "url": "logo.svg"
    },
    "description": "With this API you can retrieve information about registered organisations (includes company, business, sole trader, non-profit association etc). \n\nThe API use POST instead of GET because we don't want to display identification number (which can be considered sensitive personal data) in the URL.\n\n### Links\n\n[Download as YAML](./v1.yaml)\n\n[Download as JSON](./json)\n\n[Datamodel for the API](http://uri.suomi.fi/datamodel/ns/agent2)"
  },
  "servers": [
    {
      "url": "https://example.org/nsg/v2"
    }
  ],
  "security": [
    {
      "main_auth": [
        "orgname:read"
      ]
    }
  ],
  "tags": [
    {
      "name": "nordicinformation",
      "description": "API to retrieve data about registered organisations."
    },
    {
      "name": "isalive",
      "description": "API to control the status of the service."
    }
  ],
  "paths": {
    "/v2/registeredorganisations": {
      "post": {
        "tags": [
          "nordicinformation"
        ],
        "summary": "Retrieve data about a registered organisation",
        "description": "With this operation you can retrieve data about a registered organisation.",
        "operationId": "registeredOrganisations",
        "parameters": [
          {
            "name": "X-Request-Id",
            "in": "header",
            "description": "Client generated ID to call the API. In case of error this will be sent in the attribute `requestId` or as a header attribute.",
            "required": false,
            "style": "simple",
            "explode": false,
            "schema": {
              "type": "string"
            },
            "example": "d120654c-0d09-481a-8956-940a76474e6b"
          }
        ],
        "requestBody": {
          "$ref": "#/components/requestBodies/RegisteredOrganisationRequest"
        },
        "responses": {
          "200": {
            "$ref": "#/components/responses/RegisteredOrganisationResponse"
          },
          "400": {
            "description": "Wrong format on identifier.",
            "$ref": "#/components/responses/ErrorResponse"
          },
          "401": {
            "description": "Wrong access token.",
            "headers": {
              "X-Request-Id": {
                "description": "Identification of the API-call",
                "style": "simple",
                "explode": false,
                "schema": {
                  "type": "string"
                },
                "example": "d120654c-0d09-481a-8956-940a76474e6b"
              }
            }
          },
          "403": {
            "description": "Forbidden call, check the scope.",
            "headers": {
              "X-Request-Id": {
                "description": "ID on the call",
                "style": "simple",
                "explode": false,
                "schema": {
                  "type": "string"
                },
                "example": "d120654c-0d09-481a-8956-940a76474e6b"
              }
            }
          },
          "404": {
            "description": "Registered organisation data is missing.",
            "$ref": "#/components/responses/ErrorResponse"
          }
        },
        "security": [
          {
            "main_auth": [
              "foretagsinformation:read"
            ]
          }
        ],
        "x-auth-type": "Application & Application User",
        "x-throttling-tier": null
      }
    },
    "/id/{brisid}": {
      "get": {
        "tags": [
          "nordicinformation"
        ],
        "description": "Get basic information about a registered organisation using URI identifier. Note that URI identifier should never change. If there ever is a need to change the domain for the API gateway, original domain should still remain for redirecting the request by using permantly moved (301) response to the identifier API.",
        "summary": "Get organisation by id",
        "operationId": "getId",
        "parameters": [
          {
            "name": "brisid",
            "in": "path",
            "description": "The identifier of the organisation.",
            "required": true,
            "style": "simple",
            "explode": false,
            "schema": {
              "type": "string"
            },
            "example": 5560360793
          },
          {
            "name": "accept",
            "in": "query",
            "description": "Optional parameter for response format. Should be supported also using header-parameter. Defaults to application/json. Alternatively application/json-ld can be used.",
            "required": false,
            "style": "simple",
            "explode": false,
            "example": "application/json",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "$ref": "#/components/responses/RegisteredOrganisationResponse"
          },
          "400": {
            "description": "Wrong format on identifier.",
            "$ref": "#/components/responses/ErrorResponse"
          }
        }
      }
    },
    "/v2/isalive": {
      "get": {
        "tags": [
          "isalive"
        ],
        "summary": "API-status",
        "description": "With this operation you can check to see that the API is up and running.",
        "operationId": "isAlive",
        "responses": {
          "200": {
            "description": "OK"
          },
          "403": {
            "description": "Forbidden"
          },
          "503": {
            "description": "Server Unavailable"
          }
        },
        "security": [
          {
            "main_auth": [
              "foretagsinformation:monitor"
            ]
          }
        ],
        "x-auth-type": "Application & Application User",
        "x-throttling-tier": null
      }
    },
    "/v2/financialstatements": {
      "post": {
        "tags": [
          "nordicinformation"
        ],
        "summary": "Retrieve financial statement for the organisation",
        "description": "With this operation you can retrieve financial statement for the organisation.",
        "operationId": "financialStatements",
        "parameters": [
          {
            "name": "X-Request-Id",
            "in": "header",
            "description": "Client generated ID to call the API. In case of error this will be sent in the attribute `requestId` or as a header attribute.",
            "required": false,
            "style": "simple",
            "explode": false,
            "schema": {
              "type": "string"
            },
            "example": "d120654c-0d09-481a-8956-940a76474e6b"
          }
        ],
        "requestBody": {
          "$ref": "#/components/requestBodies/FinancialStatementRequest"
        },
        "responses": {
          "200": {
            "$ref": "#/components/responses/FinancialStatementResponse"
          },
          "400": {
            "description": "Wrong format on identifier.",
            "$ref": "#/components/responses/ErrorResponse"
          },
          "401": {
            "description": "Wrong access token.",
            "headers": {
              "X-Request-Id": {
                "description": "Identification of the API-call",
                "style": "simple",
                "explode": false,
                "schema": {
                  "type": "string"
                },
                "example": "d120654c-0d09-481a-8956-940a76474e6b"
              }
            }
          },
          "403": {
            "description": "Forbidden call, check the scope.",
            "headers": {
              "X-Request-Id": {
                "description": "ID on the call",
                "style": "simple",
                "explode": false,
                "schema": {
                  "type": "string"
                },
                "example": "d120654c-0d09-481a-8956-940a76474e6b"
              }
            }
          },
          "404": {
            "description": "Financial statement data is missing.",
            "$ref": "#/components/responses/ErrorResponse"
          }
        },
        "security": [
          {
            "main_auth": [
              "foretagsinformation:read"
            ]
          }
        ],
        "x-auth-type": "Application & Application User",
        "x-throttling-tier": null
      }
    }
  },
  "components": {
    "securitySchemes": {
      "main_auth": {
        "type": "oauth2",
        "flows": {
          "implicit": {
            "authorizationUrl": "http://example.com/api/oauth/dialog",
            "scopes": {
              "read:users": "read users info",
              "write:users": "modify or remove users"
            }
          }
        }
      },
      "api_key": {
        "type": "apiKey",
        "in": "header",
